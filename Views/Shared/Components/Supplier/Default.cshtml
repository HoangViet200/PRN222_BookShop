@model IEnumerable<Supplier>

<div class="selectBox-cont">
	@foreach (var item in Model)
	{
		<label class="custom_check w-100">
			<input type="checkbox" name="supplierId" value="@item.SupplierID" @(ViewBag.SelectedSupplierId.Contains(item.SupplierID) ? "checked" : "")>
			<span class="checkmark"></span> @item.Name
		</label>
	}
</div>